@model Dictionary<string,List<string>>
@{
    Layout = "_Layout.cshtml";
    ViewBag.Title = "检查结果";
}




<table class="table">
    <tr>
        <th>项目编号</th>
        <th>错误信息</th>
    </tr>
    @foreach (var item in Model.Keys)
    { 
        <tr>
            <td>@item</td>
            <td>@Model[item]</td>
        </tr>
    }
</table>
