@model Dictionary<string,List<string>>
@{
    Layout = "_Layout.cshtml";
    ViewBag.Title = "检查结果";
    Dictionary<string, string> Warning = ViewBag.Warning;
}




<table class="table">
    <tr>
        <th>项目编号</th>
        <th>错误信息</th>
    </tr>
    @foreach (var item in Model.Keys)
    { 
        <tr>
            <td>@item</td>
            <td>
                @foreach (var it in Model[item])
                { 
                    <p>@it</p>
                }
            </td>
        </tr>
    }
</table>


<table class="table">
    <tr>
        <th>项目编号</th>
        <th>提示</th>
    </tr>
    @foreach (var item in Warning.Keys)
    { 
        <tr>
            <td>
                @item
            </td>
            <td>@Warning[item]</td>
        </tr>
    }
</table>