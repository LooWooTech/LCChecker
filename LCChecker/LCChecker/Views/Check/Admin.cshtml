@model IEnumerable<LCChecker.Models.Detect>
@{
    ViewBag.Title = "管理员";
    string Error = "";
    if (Session["Error"] != null)
    {
        Error = Session["Error"].ToString();
        Session["Error"] = null;
    }
}

<div class="container">
    <div class="row">
        <span class="text-danger">@Error</span>
    </div>
    <div class="row">
        <div class="col-md-12">
            <table class="table table-striped">
                <tr>
                    <th>序号</th>
                    <th>行政单位</th>
                    <th>项目个数</th>
                    <th>总规模</th>
                    <th>新增耕地面积</th>
                    <th>可用于占补平衡面积</th>
                    <th>通过个数</th>
                    <th>提交次数</th>
                </tr>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Id</td>
                        <td>@Html.ActionLink(item.region as string, "DownExcel", "Check", new { fileType = "region", region = @item.region }, new { })</td>
                        <td>@item.sum</td>
                        <td>@item.totalScale</td>
                        <td>@item.AddArea</td>
                        <td>@item.available</td>
                        <td>@item.Correct</td>
                        <td>@item.submit</td>
                    </tr>  
                }
            </table>
        </div>
    </div>
    <div class="row">
        @Html.ActionLink("导出表格", "Download", "Check", new { }, new { @class = "btn btn-success" })
    </div>
</div>
